@model AirModel.Models.User
@if (User.Identity.IsAuthenticated)
{
    if (Model != null)
    {
       
    
      <li class="pull-right">  @Html.ActionLink(DiplomWork.App_LocalResources.GlobalRes.Logout, "Logout", "Autentification",new { @class = "btn btn-primary"})</li>
      @Model.Email
    }
}
else
{
    <i class="glyphicon glyphicon-user"></i>
    <li class="pull-right">@Html.ActionLink(DiplomWork.App_LocalResources.GlobalRes.Login, "Login", "Autentification",new { @class = "btn btn-primary"})</li>
    <li class="pull-right">@Html.ActionLink(DiplomWork.App_LocalResources.GlobalRes.Register, "Register", "Autentification",new { @class = "btn btn-primary"})</li>
}


